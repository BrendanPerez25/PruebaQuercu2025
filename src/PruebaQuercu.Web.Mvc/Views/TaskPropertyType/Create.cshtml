@using PruebaQuercu.Web.Startup
@using PruebaQuercu.Web.Models.PropertyType
@model TaskPropertyTypeViewModel

@{
    ViewBag.Title = L("TaskPropertyType");
    ViewBag.CurrentPageName = PageNames.PropertyType;
}

<h2>Crear Categoría</h2>

<form asp-action="Create" asp-controller="TaskPropertyType" method="post">

    <div class="form-group">
        <label for="Nombre">@L("Type property")</label>
        <input type="text" name="Description" class="form-control" placeholder="@L("Description")" maxlength="256">
    </div>
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <button type="submit" class="btn btn-primary">@L("Guardar")</button>
</form>